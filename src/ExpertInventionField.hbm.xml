<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="logic.member.ExpertInventionField" table="logic_member_expertinventionfield">
  <id column="id" name="id" type="integer">
   <generator class="increment"/>
  </id>
  <many-to-one class="logic.member.User" column="expert" name="expert"/>
  <many-to-one class="logic.invention.InventionField" column="inventionField" name="inventionField"/>
 </class>
</hibernate-mapping>
