<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="logic.member.User" table="logic_member_user">
  <id column="id" name="id" type="integer">
   <generator class="increment"/>
  </id>
  <property column="firstName" lazy="false"
   name="firstName" type="string"/>
  <property column="lastName" lazy="false"
   name="lastName" type="string"/>
  <property column="userName" lazy="false"
   name="userName" type="string"/>
  <property column="password" lazy="false"
   name="password" type="string"/>
  <property column="email" lazy="false" name="email" type="string"/>
  <property column="birthDate" lazy="false"
   name="birthDate" type="date"/>
  <property column="expert" lazy="false" name="expert" type="boolean"/>
<!--  <list name="expertInventionFieldsList" table="logic_invention_expertinventionfield">
   <key column="expert"/>
   <index column="id"/>
   <one-to-many class="logic.member.ExpertInventionField"/>
  </list> -->
 </class>
</hibernate-mapping>
