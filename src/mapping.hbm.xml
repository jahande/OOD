<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="logic.invention.Invention" table="logic_invention_invention">
  <id column="id" name="id" type="integer">
   <generator class="increment"/>
  </id>
  <property column="title" generated="never" lazy="false" name="title" type="character"/>
  <property column="totalSpec" generated="never" lazy="false"
   name="totalSpec" type="character"/>
  <property column="summary" generated="never" lazy="false"
   name="summary" type="character"/>
  <property column="ideaDescription" generated="never" lazy="false"
   name="ideaDescription" type="character"/>
  <property column="ideaHistory" generated="never" lazy="false"
   name="ideaHistory" type="character"/>
  <property column="claim" generated="never" lazy="false" name="claim" type="character"/>
  <property column="explanation" generated="never" lazy="false"
   name="explanation" type="character"/>
  <property column="file1" generated="never" lazy="false" name="file1" type="character"/>
  <property column="file2" generated="never" lazy="false" name="file2" type="character"/>
  <property column="file3" generated="never" lazy="false" name="file3" type="character"/>
  <property column="price" generated="never" lazy="false" name="price" type="long"/>
  <many-to-one class="logic.invention.InventionField" column="id" name="inventionField"/>
  <many-to-one class="logic.member.Company" column="id" name="company"/>
 </class>
 <class name="logic.invention.Change" table="logic_invention_change">
  <id column="id" name="id" type="integer"/>
 </class>
 <class name="logic.invention.InventionField" table="logic_invention_inventionfield">
  <id column="id" name="id" type="integer"/>
  <property column="name" generated="never" lazy="false" name="name" type="character"/>
 </class>
 <class name="logic.invention.InventionLog" table="logic_invention_inventionlog">
  <id column="id" name="id" type="integer"/>
 </class>
 <class name="logic.invention.InventionRegistrationRequest" table="logic_invention_inventionregistrationrequest">
  <id column="id" name="id" type="integer"/>
  <many-to-one class="logic.invention.Invention" column="id" name="invention"/>
  <many-to-one class="logic.member.User" column="id" name="assignedExpert"/>
  <property column="hasAssignedExpert" generated="never" lazy="false"
   name="hasAssignedExpert" type="boolean"/>
  <property column="sendDate" generated="never" lazy="false"
   name="sendDate" type="date"/>
  <property column="requestDate" generated="never" lazy="false"
   name="requestDate" type="date"/>
  <property column="state" generated="never" lazy="false" name="state" type="integer"/>
 </class>
 <class name="logic.invention.InvestigationLog" table="logic_invention_investigationlog">
  <id column="id" name="id" type="integer"/>
  <many-to-one class="logic.member.User" column="id" name="expert"/>
  <many-to-one class="logic.invention.InventionRegistrationRequest"
   column="id" name="request"/>
  <property column="date" generated="never" lazy="false" name="date" type="date"/>
  <property column="accepted" generated="never" lazy="false"
   name="accepted" type="boolean"/>
 </class>
 <class name="logic.invention.Share" table="logic_invention_share">
  <id column="id" name="id" type="integer"/>
  <many-to-one class="logic.member.User" column="id" name="user"/>
  <many-to-one class="logic.invention.Invention" column="id" name="invention"/>
  <property column="shareValue" generated="never" lazy="false"
   name="shareValue" type="in"/>
 </class>
 <class name="logic.member.Company" table="logic_member_company">
  <id column="id" name="id" type="integer"/>
  <property column="name" generated="never" lazy="false" name="name" type="character"/>
  <list name="agentsList" table="logic_member_user">
   <key column="id"/>
   <index column="id"/>
   <many-to-many class="logic.member.User" column="id" unique="false"/>
  </list>
 </class>
 <class name="logic.member.CompanyRegistrationRequest" table="logic_member_companyregistrationrequest">
  <id column="id" name="id" type="integer"/>
  <property column="requestDate" generated="never" lazy="false"
   name="requestDate" type="date"/>
  <property column="state" generated="never" lazy="false" name="state" type="integer"/>
 </class>
 <class name="logic.member.Manager" table="logic_member_manager">
  <id column="id" name="id" type="integer"/>
  <property column="firstName" generated="never" lazy="false"
   name="firstName" type="character"/>
  <property column="lastName" generated="never" lazy="false"
   name="lastName" type="character"/>
  <property column="userName" generated="never" lazy="false"
   name="userName" type="character"/>
  <property column="password" generated="never" lazy="false"
   name="password" type="character"/>
  <property column="email" generated="never" lazy="false" name="email" type="character"/>
  <property column="birthDate" generated="never" lazy="false"
   name="birthDate" type="date"/>
 </class>
 <class name="logic.member.User" table="logic_member_user">
  <id column="id" name="id" type="integer"/>
  <property column="firstName" generated="never" lazy="false"
   name="firstName" type="character"/>
  <property column="lastName" generated="never" lazy="false"
   name="lastName" type="character"/>
  <property column="userName" generated="never" lazy="false"
   name="userName" type="character"/>
  <property column="password" generated="never" lazy="false"
   name="password" type="character"/>
  <property column="email" generated="never" lazy="false" name="email" type="character"/>
  <property column="birthDate" generated="never" lazy="false"
   name="birthDate" type="date"/>
  <property column="expert" generated="never" lazy="false" name="expert" type="boolean"/>
  <list name="assignedInventionFields" table="logic_invention_inventionfield">
   <key column="id"/>
   <index/>
   <many-to-many class="logic.invention.InventionField" column="id" unique="false"/>
  </list>
 </class>
 <class name="logic.member.UserRegistrationRequest" table="logic_member_userregistrationrequest">
  <id column="id" name="id" type="integer"/>
  <property column="requestDate" generated="never" lazy="false"
   name="requestDate" type="date"/>
  <property column="state" generated="never" lazy="false" name="state" type="integer"/>
 </class>
</hibernate-mapping>
