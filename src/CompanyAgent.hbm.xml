<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="logic.member.CompanyAgent" table="logic_member_companyagent">
  <id column="id" name="id" type="integer">
   <generator class="increment"/>
  </id>
  <many-to-one class="logic.member.Company" column="company" name="company"/>
  <many-to-one class="logic.member.User" column="agent" name="agent"/>
 </class>
</hibernate-mapping>
