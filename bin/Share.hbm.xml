<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="logic.invention.Share" table="logic_invention_share">
  <id column="id" name="id" type="integer"/>
  <many-to-one class="logic.member.User" column="user" name="user"/>
  <many-to-one class="logic.invention.Invention" column="invention" name="invention"/>
  <property column="shareValue" lazy="false"
   name="shareValue" type="integer"/>
 </class>
</hibernate-mapping>
