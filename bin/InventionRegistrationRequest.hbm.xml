<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="logic.invention.InventionRegistrationRequest" table="logic_invention_inventionregistrationrequest">
  <id column="id" name="id" type="integer">
   <generator class="increment"/>
  </id>
  <many-to-one class="logic.invention.Invention" column="invention" name="invention"/>
  <many-to-one class="logic.member.User" column="assignedExpert" name="assignedExpert"/>
  <property column="hasAssignedExpert" lazy="false"
   name="hasAssignedExpert" type="boolean"/>
  <property column="sendDate" lazy="false"
   name="sendDate" type="date"/>
  <property column="requestDate" lazy="false"
   name="requestDate" type="date"/>
  <property column="state" lazy="false" name="state" type="integer"/>
 </class>
</hibernate-mapping>
