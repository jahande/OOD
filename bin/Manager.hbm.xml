<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="logic.member.Manager" table="logic_member_manager">
  <id column="id" name="id" type="integer"/>
  <property column="firstName" lazy="false"
   name="firstName" type="character"/>
  <property column="lastName" lazy="false"
   name="lastName" type="character"/>
  <property column="userName" lazy="false"
   name="userName" type="character"/>
  <property column="password" lazy="false"
   name="password" type="character"/>
  <property column="email" lazy="false" name="email" type="character"/>
  <property column="birthDate" lazy="false"
   name="birthDate" type="date"/>
 </class>
</hibernate-mapping>
