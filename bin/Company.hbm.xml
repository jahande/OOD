<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="logic.member.Company" table="logic_member_company">
  <id column="id" name="id" type="integer"/>
  <property column="name" name="name" type="character"/>
  <list name="agentsList" table="logic_member_companyagent">
   <key column="companyId"/>
   <index/>
   <many-to-many class="logic.member.User" column="userId" unique="false"/>
  </list>
 </class>
</hibernate-mapping>
